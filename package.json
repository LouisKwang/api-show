{
  "name": "api-show",
  "version": "0.1.16",
  "description": "api ui",
  "private": false,
  "main": "lib/apishow.umd.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "inspect": "vue-cli-service inspect",
    "lib": "cross-env NODE_ENV=production vue-cli-service build --target lib --name apishow --dest lib packages/index.js",
    "pub": "npm run lib && npm version patch &&  npm publish",
    "docs:dev": "cross-env NODE_ENV=development vuepress dev docs",
    "docs:build": "cross-env NODE_ENV=production vuepress build docs",
    "docs:serve": "cross-env NODE_ENV=development vuepress serve docs",
    "postversion": "git push --follow-tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "packages/*.js": [
      "prettier --write",
      "git add"
    ],
    "packages/*.vue": [
      "prettier --write",
      "git add"
    ],
    "packages/*.md": [
      "git add"
    ],
    "docs/*.js": [
      "prettier --write",
      "git add"
    ],
    "docs/*.md": [
      "git add"
    ]
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@api-show/vuepress-plugin-aps": "0.3.0",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vuepress/plugin-google-analytics": "^1.2.0",
    "babel-eslint": "^10.0.1",
    "core-js": "^2.6.5",
    "cross-env": "^6.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "mini-css-extract-plugin": "^0.8.0",
    "prettier": "^1.18.2",
    "vue-github-button": "^1.1.2",
    "vue-template-compiler": "^2.6.10",
    "vuepress": "^1.1.0",
    "vuepress-plugin-gitalk-maker": "^0.1.0",
    "vuepress-plugin-serve": "^1.1.0",
    "vuepress-plugin-sitemap": "2.3.1",
    "yaml": "^1.7.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "ant-design-vue": "^1.4.0",
    "api-show": "^0.1.15",
    "echarts": "^4.4.0",
    "element-ui": "^2.12.0",
    "sass": "^1.23.1",
    "sass-loader": "^8.0.0",
    "vant": "^2.2.6",
    "vue-echarts": "^4.0.4"
  }
}
